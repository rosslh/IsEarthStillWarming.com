{
  "name": "isearthstillwarming.com",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000",
    "format": "eslint --fix \"src/**/*.js\"",
    "cyTest": "cypress run",
    "buildTestServer": "react-static build && serve dist -p 3939",
    "test": "server-test buildTestServer 3939 cyTest"
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@reach/router": "^1.2.1",
    "@sentry/browser": "^4.5.2",
    "chart.js": "^2.7.3",
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.3.12",
    "react-static": "^6.3.5",
    "unitransform": "^1.0.5"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.6",
    "axios": "^0.18.0",
    "csvtojson": "^2.0.8",
    "cypress": "^3.1.4",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.0",
    "serve": "10.1.2",
    "start-server-and-test": "^1.7.11"
  }
}
